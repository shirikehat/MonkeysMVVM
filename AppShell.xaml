<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MonkeysMVVM.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MonkeysMVVM.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
            <ShellContent ContentTemplate="{DataTemplate views:MenuPage}" Icon="home.png" Route="Homepage"/>

        <Tab Icon="search.png">
            <ShellContent Title="סינון" ContentTemplate="{DataTemplate views:FindMonkeyByLocationpage}" Icon="search.png"/>
            <ShellContent Title="רענון" ContentTemplate="{DataTemplate views:MonkeysPage}"/>
        </Tab>



    </TabBar>
    
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MonkeysPage}"
        Route="MainPage" />

</Shell>

    
    